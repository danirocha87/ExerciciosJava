public class Cachorro extends Animal{       

	@Override //--> sobrescrita de método
	public void som() {	
		System.out.println("Som do cachorro: au rrau rau...");
		
	}

	@Override //--> sobrescrita de método
	public void movimento() {	
		System.out.println("Cachorro pulando e correndo...");
		
	}

	

}